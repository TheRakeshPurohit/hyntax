{
  "name": "hyntax",
  "version": "1.0.3",
  "description": "Straightforward HTML parser for Node.js and browser",
  "keywords": [
    "html",
    "parser",
    "html5",
    "html5 parser",
    "htmlparser",
    "html parser",
    "html tree-constructor",
    "html to JSON",
    "html to AST",
    "html tokenizer",
    "tokenize",
    "tokenizer",
    "stream parsing",
    "stream parser"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:nik-garmash/hyntax.git"
  },
  "homepage": "https://github.com/nik-garmash/hyntax",
  "bugs": "https://github.com/nik-garmash/hyntax/issues",
  "main": "./index.js",
  "scripts": {
    "test": "tape ${TEST:-'./tests/**/*.test.js'} | tap-spec",
    "coverage": "nyc -x 'tests/**/*' npm test",
    "generate-readme-toc": "markdown-toc --no-firsth1=true -i ./README.md",
    "prepublishOnly": "babel index.js --out-file index.es5.js && babel lib --out-dir lib-es5"
  },
  "author": {
    "name": "Nikolay Garmash",
    "email": "garmash.nikolay@gmail.com",
    "url": "https://nikgarmash.com"
  },
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=6.11.1",
    "npm": ">=5.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "deep-diff": "^0.3.8",
    "eslint": "^4.13.1",
    "markdown-toc": "^1.2.0",
    "nyc": "^11.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "dependencies": {}
}
